database schema:
  CREATE TABLE links (
    id INTEGER NOT NULL, 
    title VARCHAR(1000), 
    target VARCHAR(1000), 
    PRIMARY KEY(id)
  );
  
  CREATE TABLE words (
    id INTEGER NOT NULL, 
    term VARCHAR(255), 
    count INTEGER, 
    PRIMARY KEY(id)
  );
  